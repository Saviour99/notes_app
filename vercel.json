{
    "builds":[
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "website/static/**/*",
            "use": "@vercel/static"
        }
    ],
    "routes":[
        {
            "src": "/static/(.*)",
            "dest": "/website/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/website/views.py"
        }
    ]
}